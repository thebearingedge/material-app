{
  "name": "material-app",
  "private": true,
  "version": "0.0.0",
  "description": "Material App",
  "main": "build/index.js",
  "scripts": {
    "test:unit": "mocha -r babel-register src/**/*.unit.js",
    "test:int": "mocha -r babel-register src/**/*.int.js",
    "build:server": "babel -d build/ --ignore *.test,__test__ src/server/",
    "build:html": "pug -P src/public -o build/public",
    "build:css": "stylus src/public -o build/public",
    "build:js": "browserify -t babelify -o build/public/main.js src/client/",
    "watch:server": "babel -w -d build/ --ignore *.test,__test__ src/server/",
    "watch:html": "onchange -v 'src/public/*.pug' -- run-s -s build:html",
    "watch:css": "onchange -v 'src/public/*.styl' -- run-s -s build:css",
    "watch:js": "watchify -v -t babelify -o build/public/main.js src/client/",
    "watch:build": "run-p -s watch:server watch:html watch:css watch:js",
    "watch:start": "nodemon .",
    "db": "babel-node node_modules/.bin/knex --cwd src/db --",
    "db:make": "run-s -s 'db migrate:make {1}' --",
    "db:up": "run-s -s 'db migrate:latest'",
    "db:down": "run-s -s 'db migrate:rollback'",
    "db:cycle": "run-s -s db:down db:up",
    "lint": "eslint src/",
    "test": "run-s -s test:*",
    "tdd": "run-s -s 'test:unit -- -w -R min'",
    "clean": "rimraf build/ && mkdirp build/public/",
    "cover": "rimraf coverage/ && nyc run-s -s test",
    "build": "run-s -s clean && run-p -s build:*",
    "sync": "browser-sync start --no-open -p 'localhost:3000' -f 'build/public/'",
    "start": "node .",
    "watch": "run-s -s build && run-p -s watch:build watch:start sync",
    "precommit": "run-p -s lint build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tim Davis",
  "license": "LicenseRef-LICENSE",
  "homepage": "",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.14.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.13.0",
    "babelify": "7.3.0",
    "browser-sync": "2.16.0",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.1",
    "chai-struct": "2.0.1",
    "enzyme": "2.4.1",
    "eslint": "3.6.0",
    "eslint-plugin-react": "6.3.0",
    "global": "4.3.1",
    "husky": "0.11.8",
    "jsdom": "9.5.0",
    "karma": "1.3.0",
    "mkdirp": "0.5.1",
    "mocha": "3.0.2",
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "nyc": "8.3.0",
    "onchange": "3.0.2",
    "pug-cli": "1.0.0-alpha6",
    "react-addons-test-utils": "15.3.2",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "sinon-chai": "2.8.0",
    "stylus": "0.54.5",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "es6-error": "3.1.0",
    "express": "4.14.0",
    "express-async-wrap": "1.0.0",
    "global": "4.3.1",
    "joi": "9.0.4",
    "knex": "0.12.1",
    "material-ui": "0.15.4",
    "pg": "6.1.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "keywords": []
}
